import java.util.Scanner;
class  AlphabetReading
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Enter the number for alphabet: ");
		
		int num = sc.nextInt();
		int start = 65;
		for(int i=2;i<=num;i++){
			start++;
		}
		System.out.print((char)start);
	}
}
