import java.util.Scanner;
class HappyNumber 
{
	public static void main(String[] args) 
	{
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter the num1: ");
		int num1 = sc.nextInt();
		
		System.out.println("Enter the num2: ");
		int num2 = sc.nextInt();
		
		for(int i=num1;i<=num2;i++){
			
			if(isHappyNumber(i)){
				System.out.print(i + " ");
			}
		}
	}
	public static boolean isHappyNumber(int num){
		
		if(num == 1 || num == 7){
			return true;
		}
		if(num/10 == 0){
			return false;
		}
		
		int sum = num;
		int valueSum = 0;
		
	    while(sum > 0){
				
			int rem = sum % 10;
				
			valueSum += Math.pow(rem);
				
			sum /= 10;
				
		}
		
		return isHappyNumber(valueSum);
		
	}
}
